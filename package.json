{
  "name": "vevohub-front-end",
  "author": "Minimals",
  "version": "5.7.0",
  "description": "Vite Starter & TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node_modules/.bin/vite",
    "start": "node_modules/.bin/vite preview",
    "build": "node_modules/.bin/tsc && vite build",
    "lint": "node_modules/.bin/eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "node_modules/.bin/eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "node_modules/.bin/prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "yarn rm:all && yarn install && yarn dev",
    "re:build": "yarn rm:all && yarn install && yarn build",
    "re:build-npm": "npm run rm:all && npm install && npm run build",
    "dev:host": "node_modules/.bin/vite --host"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.3",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
